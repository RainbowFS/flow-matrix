#!/usr/bin/env python
# -*- coding: utf-8 -*-

from flask import Flask, render_template
from flowmatrix.flowlib import get_data_full, get_data

app = Flask(__name__)


@app.route("/")
def data():
    return render_template('./index.html', data=get_data())


@app.route("/full")
def data_full():
    return render_template('./index.html', data=get_data_full())
